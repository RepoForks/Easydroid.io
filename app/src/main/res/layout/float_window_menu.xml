<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="#00ffffff">
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="279dp"
    android:layout_height="279dp"
    android:background="@drawable/cornor1"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    android:orientation="vertical"
    android:clickable="true"
    >
    <RelativeLayout
        android:layout_gravity="center"
        android:layout_width="279dp"
        android:layout_height="279dp"
        android:clickable="true">
    <LinearLayout
        android:id="@+id/floatwindowmenu_first"
        android:layout_width="match_parent"
        android:layout_height="93dp"
        android:orientation="horizontal">
        <Button
            android:id="@+id/message_button"
            android:layout_width="@dimen/buttonwidth"
            android:layout_height="@dimen/buttonwidth"
            android:layout_marginLeft="@dimen/buttonfullmargin"
            android:layout_marginRight="@dimen/buttonhalfmargin"
            android:layout_marginTop="@dimen/buttonfullmargin"
            android:layout_marginBottom="@dimen/buttonhalfmargin"
            android:background="@drawable/ic_message_draw_white_48dp"
            android:gravity="center"
            android:textAllCaps="false"/>
        <Button
            android:id="@+id/call_button"
            android:layout_width="@dimen/buttonwidth"
            android:layout_height="@dimen/buttonwidth"
            android:layout_marginLeft="@dimen/buttonhalfmargin"
            android:layout_marginRight="@dimen/buttonhalfmargin"
            android:layout_marginTop="@dimen/buttonfullmargin"
            android:layout_marginBottom="@dimen/buttonhalfmargin"
            android:background="@drawable/ic_phone_white_48dp"/>
        <Button
            android:id="@+id/camera_button"
            android:layout_width="@dimen/buttonwidth"
            android:layout_height="@dimen/buttonwidth"
            android:layout_marginLeft="@dimen/buttonhalfmargin"
            android:layout_marginRight="@dimen/buttonfullmargin"
            android:layout_marginTop="@dimen/buttonfullmargin"
            android:layout_marginBottom="@dimen/buttonhalfmargin"
            android:background="@drawable/ic_camera_white_48dp"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/floatwindowmenu_second"
        android:layout_width="match_parent"
        android:layout_height="93dp"
        android:orientation="horizontal"
        android:layout_below="@id/floatwindowmenu_first">
        <Button
            android:id="@+id/connectivity_button"
            android:layout_width="@dimen/buttonwidth"
            android:layout_height="@dimen/buttonwidth"
            android:layout_marginLeft="@dimen/buttonfullmargin"
            android:layout_marginRight="@dimen/buttonhalfmargin"
            android:layout_marginTop="@dimen/buttonhalfmargin"
            android:layout_marginBottom="@dimen/buttonhalfmargin"
            android:background="@drawable/ic_swap_vertical_white_48dp"
            android:gravity="center"
            android:textAllCaps="false"/>
        <Button
            android:id="@+id/bluetooth_button"
            android:layout_width="@dimen/buttonwidth"
            android:layout_height="@dimen/buttonwidth"
            android:layout_marginLeft="@dimen/buttonhalfmargin"
            android:layout_marginRight="@dimen/buttonhalfmargin"
            android:layout_marginTop="@dimen/buttonhalfmargin"
            android:layout_marginBottom="@dimen/buttonhalfmargin"
            android:background="@drawable/ic_bluetooth_white_48dp"
            android:gravity="center"
            android:textAllCaps="false"/>
        <Button
            android:id="@+id/wifi_button"
            android:layout_width="@dimen/buttonwidth"
            android:layout_height="@dimen/buttonwidth"
            android:layout_marginLeft="@dimen/buttonhalfmargin"
            android:layout_marginRight="@dimen/buttonfullmargin"
            android:layout_marginTop="@dimen/buttonhalfmargin"
            android:layout_marginBottom="@dimen/buttonhalfmargin"
            android:background="@drawable/ic_wifi_white_48dp" />
    </LinearLayout>
        <LinearLayout
            android:id="@+id/floatwindowmenu_third"
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:orientation="horizontal"
            android:layout_below="@id/floatwindowmenu_second">
            <Button
                android:id="@+id/airplane_button"
                android:layout_width="@dimen/buttonwidth"
                android:layout_height="@dimen/buttonwidth"
                android:layout_marginLeft="@dimen/buttonfullmargin"
                android:layout_marginRight="@dimen/buttonhalfmargin"
                android:layout_marginTop="@dimen/buttonhalfmargin"
                android:layout_marginBottom="@dimen/buttonfullmargin"
                android:background="@drawable/ic_airplane_white_48dp"
                />
            <Button
                android:id="@+id/ring_button"
                android:layout_width="@dimen/buttonwidth"
                android:layout_height="@dimen/buttonwidth"
                android:layout_marginLeft="@dimen/buttonhalfmargin"
                android:layout_marginRight="@dimen/buttonhalfmargin"
                android:layout_marginTop="@dimen/buttonhalfmargin"
                android:layout_marginBottom="@dimen/buttonfullmargin"
                android:background="@drawable/ic_bell_white_48dp"
                android:gravity="center"
                android:textAllCaps="false"/>
            <Button
                android:id="@+id/home_button"
                android:layout_width="@dimen/buttonwidth"
                android:layout_height="@dimen/buttonwidth"
                android:layout_marginLeft="@dimen/buttonhalfmargin"
                android:layout_marginRight="@dimen/buttonfullmargin"
                android:layout_marginTop="@dimen/buttonhalfmargin"
                android:layout_marginBottom="@dimen/buttonfullmargin"
                android:background="@drawable/ic_home_white_48dp"/>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
</RelativeLayout>